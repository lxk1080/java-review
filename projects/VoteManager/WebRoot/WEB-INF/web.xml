<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <servlet>
    <servlet-name>CenterCL</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.vote.controller.CenterCL</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UploadUrl</servlet-name>
    <display-name>This is the display name of my J2EE component</display-name>
    <description>This is the description of my J2EE component</description>
    <servlet-class>com.vote.controller.UploadUrl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>CenterCL</servlet-name>
    <url-pattern>/CenterCL</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UploadUrl</servlet-name>
    <url-pattern>/UploadUrl</url-pattern>
  </servlet-mapping>

</web-app>